{
  "name": "webdriveriotypescripthybrid",
  "version": "1.0.0",
  "description": "Boilerplate project using webdriverio with mocha and BDD hybrid framework",
  "main": "index.js",
  "scripts": {
    "start": "ts-node runner.ts",
    "test": "wdio run ./src/config/wdio.conf.ts",
    "test:docker": "wdio run ./src/config/wdio.conf.docker.ts",
    "report:mocha": "marge ./reports/mocha/wdio-ma-merged.json && move mochawesome-report.html ./mochawesome-report",
    "report:mocha:ci": "marge ./reports/mocha/wdio-ma-merged.json",
    "test:e2e": "wdio run ./src/config/wdio.conf.e2e.ts",
    "test:e2e:docker": "wdio run ./src/config/wdio.conf.e2e.docker.ts",
    "report:cucumber": "ts-node src/generator/index.ts",
    "test:api": "mocha --timeout 20000 --reporter mochawesome --reporter-options reportDir=reports/api -r tsconfig-paths/register -r ts-node/register src/api/test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadabnepal/WebdriverIOTypeScriptHybrid.git"
  },
  "author": "MD SADAB SAQIB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sadabnepal/WebdriverIOTypeScriptHybrid/issues"
  },
  "homepage": "https://github.com/sadabnepal/WebdriverIOTypeScriptHybrid#readme",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/faker": "^5.5.9",
    "@types/supertest": "^2.0.11",
    "@wdio/cli": "^7.16.10",
    "@wdio/cucumber-framework": "^7.16.6",
    "@wdio/junit-reporter": "^7.16.6",
    "@wdio/local-runner": "^7.16.10",
    "@wdio/mocha-framework": "^7.16.6",
    "@wdio/selenium-standalone-service": "^7.16.6",
    "@wdio/spec-reporter": "^7.16.9",
    "chai": "^4.3.4",
    "cucumber-html-reporter": "^5.5.0",
    "faker": "^5.5.3",
    "mochawesome": "^7.0.1",
    "mochawesome-report-generator": "^3.1.5",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "wdio-chromedriver-service": "^7.2.2",
    "wdio-cucumberjs-json-reporter": "^4.1.4",
    "wdio-docker-service": "^3.2.0",
    "wdio-mochawesome-reporter": "^4.0.0"
  },
  "dependencies": {
    "enquirer": "^2.3.6"
  }
}
